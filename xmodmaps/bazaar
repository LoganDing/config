! use Caps Lock as Escape
clear lock
keycode 66 = Escape

! ö = slash
keycode  47 = slash odiaeresis slash odiaeresis dead_acute dead_doubleacute
! ä = return
keycode  48 = Return adiaeresis Return adiaeresis braceleft dead_caron

! (windows key = Mod4, not changing this any more)

! scroll-lock
keycode 78 = Multi_key

! eliminate some dead keys
keycode  21 = asciicircum grave asciicircum grave asciitilde ogonek
